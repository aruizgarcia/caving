issn = {19961073},
journal = {Energies},
keywords = {CFD,CHECK,Figure of merit,Hover,Isolated rotor,UVLM,Viscous coupling,Viscous effects,Vortex core growth,Wake modeling},
mendeley-tags = {UVLM,Viscous coupling,Wake modeling,CHECK},
number = {2},
pages = {1--19},
title = {{Numerical and experimental estimation of the efficiency of a quadcopter rotor operating at hover}},
volume = {12},
year = {2019}
}
