doi = {10.1007/s13272-020-00481-8},
file = {:home/alber/Documents/PhD/literature_repository/Nguyen - ISAE/Co-design of vertical tail and control laws using distributed electric propulsion.pdf:pdf},
issn = {18695590},
journal = {CEAS Aeronautical Journal},
keywords = {Aircraft Design,Aircraft handling qualities,Co-design,Differential thrust,Distributed electric propulsion,Distributed propulsion,H∞ Control design,Stability and Control,Vertical tail reduction,research_agreement},
mendeley-tags = {Aircraft Design,Distributed propulsion,Stability and Control,research_agreement},
number = {1},
title = {{Co-design of aircraft vertical tail and control laws with distributed electric propulsion and flight envelop constraints}},
year = {2020}
}
@article{Matamoros2018,
abstract = {Conventional linear control allocation (LCA) methods fail to provide satisfactory per- formance in ight control systems (FCS) for aircraft with highly nonlinear and coupled control effector suites, especially for tailless aircraft with strong interactions between con- trol effectors. This is due to the inability of linear effector models to capture nonlinear and coupled control forces and moments. This paper implements an incremental nonlin- ear control allocation (INCA) approach, which can capture nonlinearities and interactions of control effectors while being solvable with simple and computationally effcient linear control allocation algorithms. This makes INCA suitable for real-time control allocation in FCS. This incremental reformulation of the control allocation problem is based on a Jacobian model of the control eFFectors, and relies on angular acceleration measurements to reduce model dependency. In addition, it operates based on real-time measurements of the actuator positions, which mitigates typical problems related to couplings between control allocators and actuator dynamics. In this paper, LCA- and INCA-based nonlinear FCS are designed for the Innovative Control Effectors (ICE) aircraft, a highly maneuver- able tailless aircraft with 13 highly nonlinear, interacting and axis-coupled control effectors. Real-time simulation results show that INCA dramatically improves tracking and control allocation performance, thus improving maneuverability and exploiting the full potential of innovative control effector suites.},
author = {Matamoros, I. and de Visser, C. C.},
doi = {10.2514/6.2018-1116},
file = {:home/alber/Documents/PhD/literature_repository/Control design/Control and Simulation/Incremental Nonlinear Control Allocation for a Tailless Aircraft with Innovative Control Effectors.pdf:pdf},
isbn = {9781624105265},
journal = {AIAA Guidance, Navigation, and Control Conference, 2018},
keywords = {Control allocation,Control design,Simplex splines,research_agreement},
mendeley-tags = {Control allocation,Control design,Simplex splines,research_agreement},
number = {210039},
pages = {1--25},
title = {{Incremental nonlinear control allocation for a tailless aircraft with innovative control effectors}},
year = {2018}
}
@phdthesis{Winckelmans_PhD,
abstract = {Contributions to vortex methods for the computation of incompressible\nunsteady flows are presented. Three methods are investigated. The\nfirst method is the inviscid method of vortex filaments in three\ndimensions, and the following topics are presented: review of the\nmethod of regularized vortex filaments and of convergence results\nfor multiple-filament computations; modeling of a vortex tube by\na single filament convected with the regularized Biot-Savart velocity\napplied on the centerline; and development of a new regularization\nof the Biot-Savart law that reproduces the lowest mode dispersion\nrelation to the rectilinear vortex tube in the range of large to\nmedium wavelengths. Next the method of vortex particles in three\ndimensions is investigated, and the following contributions are discussed:\nreview of the method of singular vortex particles; review of the\nmethod of regularized vortex particles and of convergence results,\nand introduction of a new algebraic smoothing with convergence properties;\ndevelopment of a new viscous method in which viscous diffusion is\ntaken into account by a scheme that redistributes the particle strength\nvectors, and application of the method to the computation of the\nfusion of two vortex rings; investigation of the particle method\nwith respect to the conservation laws and derivation of expressions\nfor the evaluation of the quadratic diagnostics. The third method\nconsidered is the method of contour dynamics in two dimensions. The\nparticular efforts presented are: review of the classical inviscid\nmethod and development of a new vector particle version of the method,\nboth singular and regularized; development of a viscous version of\nthe method of regularized particles and application of the method\nto computation of the reconnection of two vortex patches of same\nsign vorticity; and investigation of the particle method with respect\nto the conservation laws and derivation of new expressions for the\nevaluation of linear and quadratic diagnostics.},
author = {Winckelmans, Gregoire Stephane},
file = {:home/alber/Documents/PhD/literature_repository/Numerical aerodynamics/UVLM/Topics in vortex methods for the computation of three and two incompressible unsteady flows.pdf:pdf},
keywords = {PhD Theses,Thesis,Vortex particles},
mendeley-tags = {PhD Theses,Vortex particles,Thesis},
school = {California Institute of Technology},
title = {{Topics in Vortex Methods for the Computation of Three-and Two-dimensional Incompressible Unsteady Flows}},
year = {1989}
}
@article{PerezGordillo2019,
abstract = {Globalization has led to an increase in the use of small copters for different activities such as geo-referencing, agricultural fields monitoring, survillance, among others. This is the main reason why there is a strong interest in the performance of small-scale propellers used in unmanned aerial vehicles. The flow developed by rotors is complex and the estimation of its aerodynamic performance is not a trivial process. In addition, viscous effects, when the rotor operates at low Reynolds, affect its performance. In the present paper, two different computational methods, Computational Fluid Dynamics (CFD) and the Unsteady Vortex Lattice Method (UVLM) with a viscous correction, were used to study the performance of an isolated rotor of a quadcopter flying at hover. The Multi Reference Frame model and transition SST $\kappa$ − $\omega$ turbulence model were used in the CFD simulations. The tip vortex core growth was used to account for the viscous effects in the UVLM. The wake structure, pressure coefficient, thrust and torque predictions from both methods are compared. Thrust and torque results from simulations were validated by means of experimental results of a characterization of a single rotor. Finally, figure of merit of the rotor is evaluated showing that UVLM overestimates the efficiency of the rotor; meanwhile, CFD predictions are close to experimental values.},
author = {{P{\'{e}}rez Gordillo}, Andres M. and Santos, Juan Sebastian Villegas and {Lopez Mejia}, Omar D. and Collazos, Laura Juliana Su{\'{a}}rez and Escobar, Jaime A.},
