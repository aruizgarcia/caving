author = {Hosangadi, Pranav and Gopalarathnam, Ashok},
doi = {10.2514/1.c036027},
eprint = {2006.00107},
file = {:home/alber/Documents/PhD/literature_repository/Numerical aerodynamics/VLM/Decambering/A Low-Order Method for Prediction of Separation and Stall on Unswept Wings.pdf:pdf},
