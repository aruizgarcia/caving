issn = {00845906},
number = {1077},
pages = {1--63},
title = {{The theory and practice of estimating the accuracy of dynamic flight-determined coefficients}},
type = {NASA Reference Publication},
url = {http://www.google.com/url?sa=t{\&}rct=j{\&}q=he theory and practice of estimating the accuracy of dynamic{\%}5Ctflight-determined{\%}5Ctcoefficients{\%}22,{\&}source=web{\&}cd=1{\&}cad=rja{\&}ved=0CDgQFjAA{\&}url=http://www.nasa.gov/centers/dryden/pdf/87937main{\_}H-1128.pdf{\&}ei=647cUIu9E},
