file = {:home/alber/Dropbox/Thesis/Literature Study/Literature Repository/System Identification/Aerodynamic Model Structure/Klein, Noderer - Modeling of Aircraft Unsteady Aerodynamic Characteristics - Part 1$\backslash$: Postulated Models.pdf:pdf},
